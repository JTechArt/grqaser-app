{
  "name": "grqaser",
  "version": "1.0.0",
  "description": "Grqaser - Audiobook mobile application for Armenian literature",
  "main": "index.js",
  "scripts": {
    "android": "cd GrqaserApp && react-native run-android",
    "ios": "cd GrqaserApp && react-native run-ios",
    "start": "cd GrqaserApp && react-native start",
    "dev": "cd GrqaserApp && react-native start",
    "test": "jest",
    "admin:start": "cd books-admin-app && npm start",
    "admin:test": "cd books-admin-app && npm test",
    "admin:dev": "cd books-admin-app && npm run dev",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace Grqaser.xcworkspace -scheme Grqaser -configuration Release -destination generic/platform=iOS -archivePath Grqaser.xcarchive archive",
    "bmad:refresh": "bmad-method install -f -i codex",
    "bmad:list": "bmad-method list:agents",
    "bmad:validate": "bmad-method validate"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/material-top-tabs": "^6.6.5",
    "react-native-screens": "^3.27.0",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-reanimated": "^3.5.4",
    "react-native-track-player": "^4.0.1",
    "@reduxjs/toolkit": "^1.9.7",
    "react-redux": "^8.1.3",
    "axios": "^1.5.0",
    "react-native-vector-icons": "^10.0.2",
    "react-native-fast-image": "^8.6.3",
    "@react-native-async-storage/async-storage": "^1.19.5",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-sound": "^0.11.2",
    "react-native-slider": "^0.11.0",
    "react-native-modal": "^13.0.1",
    "react-native-paper": "^5.11.1",
    "react-native-elements": "^3.4.3",
    "react-native-ratings": "^8.1.0",
    "react-native-search-bar": "^3.4.2",
    "react-native-pull-to-refresh": "^2.1.0",
    "react-native-super-grid": "^5.1.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-permissions": "^3.10.1",
    "react-native-background-timer": "^2.4.1",
    "react-native-keep-awake": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@types/react-native": "^0.72.2",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/jest": "^29.5.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "keywords": [
    "react-native",
    "audiobooks",
    "armenian",
    "literature",
    "mobile-app"
  ],
  "author": "Grqaser Team",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/grqaser.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/grqaser/issues"
  },
  "homepage": "https://github.com/your-username/grqaser#readme"
}
