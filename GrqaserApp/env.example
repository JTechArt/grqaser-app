# GrqaserApp Environment Configuration
# Copy this file to .env and customize the values

# API Configuration
API_BASE_URL=https://grqaser.org/api
API_TIMEOUT=30000

# App Configuration
APP_NAME=Grqaser
APP_VERSION=1.0.0
BUILD_NUMBER=1

# Development Settings
DEBUG_MODE=true
ENABLE_LOGGING=true
ENABLE_ANALYTICS=false

# Audio Configuration
AUDIO_CACHE_SIZE=500MB
MAX_DOWNLOAD_SIZE=1GB

# Optional: Override for development
# API_BASE_URL=http://localhost:3000/api
# DEBUG_MODE=true
